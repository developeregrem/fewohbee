<form id="entry-form-new" role="form" method="POST"
      data-controller="settings"
      data-action="submit->settings#submitFormAction"
      data-url="{{ path('reservationorigin.create.origin') }}"
      data-success-url="{{ path('reservationorigin.overview') }}">
    <div class="modal-body ps-5 pe-5">
        <div id="flash-message-overlay">
            <div class="col">
            </div>
        </div>
        {% include 'ReservationOrigin/reservationorigin_form_input_fields.html.twig' %}
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">{{ 'button.cancel'|trans }}</button>
        <input type="submit" value="{{ 'button.save'|trans }}" class="btn btn-primary"/>
    </div>
</form>
